{
  "id": null,
  "title": "GenAI Ops Overview",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 12,
  "panels": [
    {
      "type": "graph",
      "title": "p95 Workflow Latency (seconds)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(workflow_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 latency"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Total Workflow Runs",
      "targets": [
        { "expr": "round(increase(workflow_runs_total[5m]))" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 }
    },
    {
      "type": "stat",
      "title": "Total Workflow Errors",
      "targets": [
        { "expr": "round(increase(workflow_errors_total[5m]))" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 }
    },
    {
      "type": "stat",
      "title": "Active Workflow Threads",
      "targets": [
        { "expr": "workflow_active_threads" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 8 }
    },
    {
      "type": "stat",
      "title": "Total Workflow Tokens (last 5m)",
      "targets": [
        { "expr": "round(increase(workflow_total_tokens_sum[5m]))" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 }
    },
    {
      "type": "stat",
      "title": "Avg Prompt Tokens",
      "targets": [
        { "expr": "rate(workflow_prompt_tokens_sum[5m]) / rate(workflow_prompt_tokens_count[5m])" }
      ],
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 12 }
    },
    {
      "type": "stat",
      "title": "Avg Completion Tokens",
      "targets": [
        { "expr": "rate(workflow_completion_tokens_sum[5m]) / rate(workflow_completion_tokens_count[5m])" }
      ],
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 12 }
    },
    {
      "type": "stat",
      "title": "Avg Total Tokens",
      "targets": [
        { "expr": "rate(workflow_total_tokens_sum[5m]) / rate(workflow_total_tokens_count[5m])" }
      ],
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 12 }
    },
    {
      "type": "graph",
      "title": "Agent Latency (p95 seconds)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(agent_latency_seconds_bucket[5m])) by (agent_name, le))",
          "legendFormat": "{{agent_name}}"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 }
    },
    {
      "type": "table",
      "title": "Agent Runs (last 5m)",
      "targets": [
        {
          "expr": "round(increase(agent_runs_total{agent_name=~\"flight_agent|hotel_agent\"}[5m]))",
          "legendFormat": "{{agent_name}}"
        }
      ],
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 24 }
    },
    {
      "type": "table",
      "title": "Agent Errors (last 5m)",
      "targets": [
        {
          "expr": "round(increase(agent_errors_total{agent_name=~\"flight_agent|hotel_agent\"}[5m]))",
          "legendFormat": "{{agent_name}}"
        }
      ],
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 24 }
    },
    {
      "type": "graph",
      "title": "Agent Tokens Used (avg last 5m)",
      "targets": [
        {
          "expr": "rate(agent_tokens_used_sum[5m]) / rate(agent_tokens_used_count[5m])",
          "legendFormat": "{{agent_name}} avg tokens"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 30 }
    },
    {
      "type": "stat",
      "title": "Orchestrator Uptime",
      "targets": [
        { "expr": "up{job=\"orchestrator\"}" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 38 }
    },
    {
      "type": "stat",
      "title": "Orchestrator Health",
      "targets": [
        { "expr": "orchestrator_health" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 38 }
    },
    {
      "type": "stat",
      "title": "Workflow CPU Usage (%)",
      "targets": [
        { "expr": "workflow_cpu_usage_percent" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 38 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto",
        "unit": "percent"
      }
    },
    {
      "type": "stat",
      "title": "Workflow RAM Usage (MB)",
      "targets": [
        { "expr": "workflow_ram_usage_mb" }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 38 }
    },
    {
      "type": "table",
      "title": "Evaluation Summary (Best per Agent)",
      "targets": [
        {
          "expr": "eval_summary",
          "legendFormat": "{{agent_name}} | {{best_model}} | {{best_prompt_version}} | {{metric}}"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 42 },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "labels"
          }
        }
      ]
    }
  ]
}
